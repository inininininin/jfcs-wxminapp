.talkBox{
  width: 686rpx;
/* height: 153rpx; */
padding: 0 0 28rpx;
background: #ffffff;
border-radius: 20rpx;
margin: 50rpx 32rpx 20rpx;
box-sizing: border-box;
}
.talkBoxTop{
  width: 100%;
  height: 48rpx;
  padding-top: 28rpx;
  line-height: 48rpx;
}
.talkBoxTop >view{
  width: 48rpx;
  height: 48rpx;
  margin: 0 20rpx 0 28rpx;
  border-radius: 50%;
  background: #FFD37C;
  overflow: hidden;
  display: inline-block;
  float: left;
}
.talkBoxTop >view image{
 width: 31rpx;
 height: 32rpx;
 margin: 9rpx 9rpx 8rpx 9rpx;
}
