.tabbarBtn{
  width: 750rpx;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #ffffff;
}
.tabbarBtn>button:not([size='mini']){
  padding: 0;
  width: 678rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #f5f5f5;
border-radius: 30px;
margin: 20rpx 36rpx;
}
.tabbarBtn>button::after{
  width: 678rpx;
height: 60rpx;
background: #f5f5f5;
border-radius: 30px;
margin: 20rpx 36rpx;
}
.tabbarBtn>button text{
font-size: 26rpx;
font-family: PingFangSC, PingFangSC-Regular;
font-weight: 400;
text-align: left;
color: #999999;
line-height: 60rpx;
letter-spacing: -1rpx;
margin-left: 30rpx;
}
.tabbarBtn>button image{
  width: 60rpx;
  height: 60rpx;
  float: right;
}

.question{
  width: 678rpx;
  height: auto;
  background: #ffffff;
  margin: 0 36rpx 20rpx;
}
.question>view{
  width: 678rpx;
  height: auto;
  margin: 0 0 20rpx;
  background: #ffffff;
  border-radius: 5rpx;
  box-sizing: border-box;
  padding: 20rpx 26rpx;
}
.question>view text{
  display:block;
  width: auto;
  padding: 10rpx 20rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #0050cc;
  word-break:break-all;word-wrap:break-word
}
.question>view text:nth-child(1){
  line-height: 40rpx;
  font-size: 28rpx;
  color: #999999;
}


