}
.programmeDetail{
  margin-top: 45rpx;
  height: 68rpx;
  width: 100%;
}
.programmeDetail>view{
  width: 374rpx;
  text-align: center;
  display: inline-block;
}
.programmeDetail>view:nth-child(1){
  border-right: 2rpx solid #999999;
}
.programmeDetail>view text{
  padding: 0 30rpx;
  display: block;
height: 33rpx;
opacity: 0.9;
font-size: 24rpx;
