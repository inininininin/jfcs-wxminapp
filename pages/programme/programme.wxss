/* pages/programme/programme.wxss */
.container {
  box-sizing: border-box;
  position: relative;
  padding: 0 !important;
  background: #EDEDED;
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #EDEDED;
  z-index: 2;
  box-shadow: 0px 1px 4px 0px rgba(0,0,0,0.1);
}

.title-bar {
  display: flex;
  align-items: center;
}

.title-bar>view {
  height: 60rpx;
  width: 460rpx;
  background: #fff;
  border-radius: 30rpx;
  float: left;
  line-height: 60rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.back {
  width: 18rpx;
  height: 30rpx;
  display: inline-block;
  padding: 20rpx;
  margin-left: 12rpx;
}

.evahospital {
  width: 610rpx;
  text-align: center;
  opacity: 1;
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  letter-spacing: 0rpx;
}


page{
  background: #F5F5F5;
}
.topbg{
  width: 750rpx;
  height: 388rpx;
  /* opacity: 0.5; */
  background: #5f4e4e;
  box-sizing: border-box;
  background-size: 100% 100%;
}
.name{
  padding-top: 27rpx;
  margin: 0 0 4rpx 50rpx;
height: 56rpx;
font-size: 40rpx;
font-family: PingFangSC, PingFangSC-Medium;
font-weight: 500;
text-align: left;
color: #ffffff;
line-height: 56rpx;
}
.age{
height: 42rpx;
font-size: 30rpx;
font-family: PingFangSC, PingFangSC-Regular;
font-weight: 400;
text-align: left;
color: #ffffff;
line-height: 42rpx;
}
.age text{
  display: inline-block;
