position: fixed;
right: 24rpx;
bottom: 289rpx;
border-radius: 50%;
overflow:hidden;
}
.kefu image{
  width: 55rpx;
  height: 57rpx;
  margin: 9rpx  22rpx 0;
  display: block;
  /* color: #333333; */
