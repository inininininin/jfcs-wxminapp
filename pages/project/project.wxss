/* pages/project/project.wxss */
page{
  background: #f7f8fa;
}
.topBox {
  width: 750rpx;
  height: 482rpx;
  background: #f7f8fa;
  position: relative;
}

.topBox .topbg {
  width: 750rpx;
  height: 443rpx;
  background: url(http://39.99.246.175:81/jfcs/wxminapp/topbg.png) no-repeat;
  background-size: 100% 100%;
}

.container {
  box-sizing: border-box;
  position: relative;
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  /* background: #fff; */
  /* z-index: 4; */
  /* opacity: 0.5; */
  background:  url(http://39.99.246.175:81/jfcs/wxminapp/topbg.png) no-repeat;
  background-size: 100% ;
}

.title-bar {
  display: flex;
  align-items: center;
}

.callSis {
  width: 200rpx;
  height: 100%;
}

.callSis image {
  width: 44rpx;
  height: 46rpx;
  margin-left: 28rpx;
  vertical-align: middle;
}

.callSis text {
  /* width: 55px; */
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: center;
  /* color: #ffe3ab; */
  line-height: 40rpx;
  letter-spacing: -1rpx;
  margin-left: 6rpx;
}

.topvip {
  position: absolute;
  bottom: 7rpx;
  left: 28rpx;
  width: 694rpx;
  height: 317rpx;
  background: url(http://39.99.246.175:81/jfcs/wxminapp/VIP.svg) no-repeat;
  background-size: 100% 100%;
  z-index: 2;
}

.topyy {
  width: 636rpx;
  height: 171rpx;
  position: absolute;
  bottom: 0;
  left: 58rpx;
  background: rgba(217, 217, 217, 0.6);
  border-radius: 0px 0px 16px 16px;
  filter: blur(10px);
  z-index: 1;
}

.topvip>view:nth-child(1) {
  width: 100%;
  height: 98rpx;
  margin-top: 30rpx;
}

.topvip>view:nth-child(1) image {
  width: 98rpx;
  height: 98rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  background: #ffffff;
  float: left;
}

.topvip>view:nth-child(1) text {
  height: 98rpx;
  font-size: 38rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  text-align: left;
  color: #464751;
  line-height: 98rpx;
}

.topvip>view:nth-child(2) {
  width: 600rpx;
  height: 74rpx;
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #464751;
  line-height: 37rpx;
  margin-left: 47rpx;
  margin-top: 52rpx;
}

.projectTitle {
  width: 100%;
  height: auto;
  text-align: center;
}

.projectTitleLf {
  width: 16rpx;
  height: 28rpx;
  display: inline-block;
}

.projectTitleRt {
  width: 16rpx;
  height: 28rpx;
  display: inline-block;
}

.projectTitleMd {
  display: inline-block;
  margin: 33rpx 30rpx 38rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  text-align: left;
  color: #333333;
  line-height: 50rpx;
}
